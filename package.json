{
  "name": "discord-botstrap",
  "version": "0.1.0",
  "description": "This is a full Typescript template for creating a Discord bot. The codebase uses the common and best practices for linting, formatting and design patterns.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.8.0",
  "dependencies": {
    "@types/jest": "^29.5.14",
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0",
    "jest": "^29.7.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "ts-jest": "^29.3.2",
    "tslib": "^2.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  }
}
